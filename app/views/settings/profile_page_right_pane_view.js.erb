<%if @right_pane_view == 'analytics'%>
	document.getElementById('profile_page_right_pane_partail_id').innerHTML = "<%= j render 'settings/profile_page/profile_page_right_pane_analytics'%>";
	document.getElementById('settings_profile_page_right_pane_view_link_analytics').classList.add('question_highlight');
	document.getElementById('settings_profile_page_right_pane_view_link_profile_details').classList.remove('question_highlight');
	document.getElementById('settings_profile_page_right_pane_view_link_settings').classList.remove('question_highlight');

<%elsif @right_pane_view == 'profile'%>
	document.getElementById('profile_page_right_pane_partail_id').innerHTML = "<%= j render 'settings/profile_page/profile_page_right_pane_profile_details'%>";
	document.getElementById('settings_profile_page_right_pane_view_link_analytics').classList.remove('question_highlight');
	document.getElementById('settings_profile_page_right_pane_view_link_profile_details').classList.add('question_highlight');
	document.getElementById('settings_profile_page_right_pane_view_link_settings').classList.remove('question_highlight');

<%elsif @right_pane_view == 'settings'%>
	document.getElementById('profile_page_right_pane_partail_id').innerHTML = "<%= j render 'settings/profile_page/profile_page_right_pane_settings'%>";
	document.getElementById('settings_profile_page_right_pane_view_link_analytics').classList.remove('question_highlight');
	document.getElementById('settings_profile_page_right_pane_view_link_profile_details').classList.remove('question_highlight');
	document.getElementById('settings_profile_page_right_pane_view_link_settings').classList.add('question_highlight');

<%end%>